config_server = server:/etc/openvpn/server/server.conf : CONTAINS : local 192.168.1.3
create_cert_key_root = server:openssl.stdout : CONTAINS : CA:TRUE
x509_extension = server:/etc/openvpn/server/x509-extensions.cnf : CONTAINS : extendedKeyUsage = clientAuth
sign_server = server:openssl.stdout : CONTAINS : O = PTIT, CN = SERVER
sign_client = server:openssl.stdout : CONTAINS : O = PTIT, CN = CLIENT
create_tc_key = server:/etc/openvpn/server/tc.key : CONTAINS : -----BEGIN OpenVPN Static key V1----- 
config_client = openvpn:/etc/openvpn/client/client.conf : CONTAINS : remote 192.168.1.3 
connection = openvpn:ping.stdout : CONTAINS : 64 bytes from 10.8.0.1:
